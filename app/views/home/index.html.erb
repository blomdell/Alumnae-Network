<%= render 'shared/header' %>
<div id="wrapper" style="position: relative; margin-top: 100px; width: 100%; text-align:center">
	
	<div id="signup" style="float: left; width: 200px; height: 200px; border: 1px solid #999; text-align:left; border-radius: 5px; font-size: 12px; font-family: tahoma; background: #999"> 
		<form>
			LOGO
		</form>
	</div>
	<div id="login" style="float: left; margin-left: 10px; width: 200px; height: 200px; border: 1px solid #999; text-align:left; border-radius: 5px; font-size: 12px; font-family: tahoma; background: #999""> 
		<% if current_user %>
			<%= link_to "Show all users", users_path %> <br>
			<%= link_to 'Show discussions', posts_path %> <br>
			Calendar <br>
			Jobs <br>
			News <br>
			Contact <br>
			
		<% else %>
			Welcome to our Alumnae Network, please sign in
		<% end %>
	</div>
</div>


<%= render 'shared/footer' %>